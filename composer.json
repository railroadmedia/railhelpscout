{
  "name": "railroad/railhelpscout",
  "type": "library",
  "description": "Wrapper package to integrate helpscout into railroad platforms",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/railroadmedia/helpscout-api-php"
    }
  ],
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "helpscout/api": "dev-master as v1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\RailHelpScout\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\RailHelpScout\\Tests\\": "tests/"
    }
  }
}
