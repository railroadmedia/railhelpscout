{
  "name": "railroad/railhelpscout",
  "type": "library",
  "description": "Wrapper package to integrate helpscout into railroad platforms",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "railroadmedia/helpscout-api-php": "~3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\RailHelpScout\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\RailHelpScout\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
