{
  "name": "railroad/railhelpscout",
  "type": "library",
  "description": "Wrapper package to integrate helpscout into railroad platforms",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "ext-curl": "*",
    "ext-json": "*",
    "helpscout/api": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\RailHelpScout\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\RailHelpScout\\Tests\\": "tests/"
    }
  }
}
